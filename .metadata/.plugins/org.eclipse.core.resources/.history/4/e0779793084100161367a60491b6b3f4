#include <iostream>
#include "src/CShape.h"
#include "gtest/gtest.h"
#include "boost/for_each.hpp"

using namespace std;

TEST(CShape, getID){
	// arrange
	CShape obj;
	int rcVal;
	int expVal = 1;

	// act
	rcVal = obj.GetID();

	// assert
	EXPECT_EQ(expVal, rcVal);
}

TEST(Boost, FOR_EACH){
	// arrange
	CShape obj;
	int rcVal;
	int expVal = 1;

	// act
	rcVal = obj.GetID();

	// assert
	EXPECT_EQ(expVal, rcVal);
}


int main(int argc, char ** argv)
{
	::testing::InitGoogleTest(&argc, argv);
	return RUN_ALL_TESTS();
}

