#include <iostream>
#include "src/CShape.h"
#include "gtest/gtest.h"

using namespace std;

TEST(CShape, getID){
	// arrange
	CShape obj;
	int rcVal;
	int expVal = 1;

	// act
	rcVal = obj.GetID();

	// assert
	EXPECT_EQ(expVal, rcVal);
}

